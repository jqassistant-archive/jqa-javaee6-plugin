<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.10"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.10 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.10.xsd"
                    name="jQAssistant Java EE 6 Plugin" id="jqa.plugin.javaee">
    <description>Aggregates plugins providing scanners and rules for Java EE 6 technologies (e.g. JPA2, EJB3).</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.EnterpriseApplicationArchiveDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.WebApplicationArchiveDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.WebApplicationDirectoryDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.WebXmlDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.SessionConfigDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ErrorPageDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ServletDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ServletMappingDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.UrlPatternDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.IconDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.DescriptionDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.DisplayNameDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ParamValueDescriptor</class>
