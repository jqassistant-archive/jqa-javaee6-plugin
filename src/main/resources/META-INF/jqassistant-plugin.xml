        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.SecurityRoleRefDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.FilterDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.FilterMappingDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.DispatcherDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ListenerDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.SecurityConstraintDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.AuthConstraintDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.UserDataConstraintDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.RoleNameDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.WebResourceCollectionDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.HttpMethodDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.HttpMethodOmissionDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.SecurityRoleDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.LoginConfigDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.FormLoginConfigDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ApplicationXmlDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.EjbModuleDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ConnectorModuleDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.ClientModuleDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.WebModuleDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.JsfFaceletDescriptor</class>
    </model>
    <scope>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.scanner.WebApplicationScope</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.scanner.EnterpriseApplicationScope</class>
    </scope>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.EarArchiveScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.WebApplicationArchiveScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.WebApplicationDirectoryScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.WarClassesFileScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.WarClassesDirectoryScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.WebXmlScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.ApplicationXmlScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.JsfFaceletScannerPlugin</class>
    </scanner>
